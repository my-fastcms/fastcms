<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fastcms.hello.HelloPluginMapper">

    <select id="getHelloList" resultType="com.fastcms.hello.Hello">
        select * from test
    </select>

    <select id="findByHelloId" resultType="com.fastcms.hello.HelloService$HelloVo">
        select * from test where id = #{id}
    </select>

</mapper>
